<Window x:Class="Group3.Semester3.DesktopClient.UploadFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Group3.Semester3.DesktopClient"
        mc:Ignorable="d"
        Title="UploadFile" Height="450" Width="800">
    <Grid>
        <DockPanel Margin="100">
            <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Top">
                <Button Name="btnUploadFile" Click="btnUploadFiles_Click">Upload files</Button>
                <Button Name="btnSubmit" Click="btnSubmit_Click" Margin="20,0,0,0">Upload</Button>
            </WrapPanel>
            <ListBox Name="lbFiles" />
        </DockPanel>
    </Grid>
</Window>
